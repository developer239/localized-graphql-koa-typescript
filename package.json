{
  "name": "localized-graphql-koa-typescript",
  "version": "1.0.0",
  "author": {
    "name": "Michal Jarnot",
    "email": "m.jarnot@yahoo.com"
  },
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && node_modules/.bin/babel src --out-dir build --copy-files --extensions '.js,.ts'",
    "dev": "node_modules/.bin/nodemon -L --exec \"node_modules/.bin/babel-node --extensions '.js,.ts' src/index.ts\"",
    "db:migrate": "node_modules/.bin/knex migrate:latest",
    "db:rollback": "node_modules/.bin/knex migrate:rollback",
    "db:seed": "node_modules/.bin/knex seed:run",
    "test": "node_modules/.bin/jest -i --forceExit --detectOpenHandles",
    "test:coverage": "node_modules/.bin/jest -i --forceExit --detectOpenHandles --coverage",
    "heroku-postbuild": "npm run build && npm run db:rollback && npm run db:migrate && npm run db:seed",
    "prod": "node build"
  },
  "type-check": "tsc",
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write --loglevel warn",
      "tslint --force --fix",
      "git add"
    ]
  },
  "greenkeeper": {
    "ignore": [
      "apollo-server-koa"
    ]
  },
  "dependencies": {
    "apollo-server-koa": "1.3.6",
    "dotenv": "6.0.0",
    "graphql": "14.0.2",
    "graphql-errors": "2.1.0",
    "graphql-playground-middleware-koa": "1.6.4",
    "graphql-tools": "4.0.0",
    "ip": "1.1.5",
    "knex": "0.15.2",
    "koa": "2.5.3",
    "koa-bodyparser": "4.2.1",
    "koa-compose": "4.1.0",
    "koa-router": "7.4.0",
    "koa-sslify": "2.1.2",
    "objection": "1.3.0",
    "pg": "7.4.3",
    "ramda": "0.25.0",
    "slugify": "1.3.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/plugin-proposal-json-strings": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-syntax-import-meta": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/dotenv": "4.0.3",
    "@types/faker": "4.1.4",
    "@types/graphql": "14.0.0",
    "@types/ip": "0.0.31",
    "@types/jest": "23.3.2",
    "@types/knex": "0.14.24",
    "@types/koa": "2.0.46",
    "@types/koa-bodyparser": "5.0.1",
    "@types/koa-compose": "3.2.2",
    "@types/koa-router": "7.0.32",
    "@types/node": "10.10.3",
    "@types/ramda": "0.25.38",
    "@types/sinon": "5.0.2",
    "@types/supertest": "2.0.6",
    "@types/uuid": "3.4.4",
    "babel-plugin-module-resolver": "3.1.1",
    "faker": "4.1.0",
    "husky": "0.14.3",
    "jest": "23.6.0",
    "lint-staged": "7.3.0",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "sinon": "6.3.4",
    "supertest": "3.3.0",
    "ts-jest": "23.1.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "typescript": "3.0.1"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "keywords": [
    "apollo",
    "apollo-server-koa",
    "dotenv",
    "graphql",
    "graphql-errors",
    "graphql-playground",
    "graphql-playground-middleware-koa",
    "graphql-tools",
    "ip",
    "knex",
    "koa",
    "koa-bodyparser",
    "koa-compose",
    "koa-router",
    "objection",
    "pg",
    "ramda",
    "slugify",
    "uuid",
    "faker",
    "jest",
    "nodemon",
    "prettier",
    "sinon",
    "ts-jest",
    "ts-node",
    "tslint",
    "tslint",
    "tslint-config-prettier",
    "typescript",
    "localized",
    "multilanguage",
    "translations"
  ]
}
